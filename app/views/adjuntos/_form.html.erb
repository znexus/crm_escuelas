<%= error_messages_for :adjunto %>

<% form_for(:adjunto, :url => adjuntos_path, 
                      :html => { :multipart => true }) do |f| -%>
  <p>
    <label for="adjunto">Subir adjunto:</label>
    <%= f.file_field :uploaded_data %>
  </p>
  <p>
    <%= submit_tag 'Subir' %>
  </p>
<% end -%>

